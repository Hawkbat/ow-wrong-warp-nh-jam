<DialogueTree xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/xen-42/outer-wilds-new-horizons/main/NewHorizons/Schemas/dialogue_schema.xsd">
	<NameField>Ernesto</NameField>

	<DialogueNode>
		<Name>START</Name>
		<EntryCondition>DEFAULT</EntryCondition>
		<Dialogue>
			<Page>Hey it's me Ernesto the magic talking anglerfish.</Page>
		</Dialogue>

		<DialogueOptionsList>
			<DialogueOption>
				<Text>Wow, the real Ernesto! I'm a huge fan!</Text>
				<DialogueTarget>1</DialogueTarget>
			</DialogueOption>
			<DialogueOption>
				<Text>How did you get here?</Text>
				<DialogueTarget>2</DialogueTarget>
			</DialogueOption>
			<DialogueOption>
				<Text>I'll see you around.</Text>
				<DialogueTarget>3</DialogueTarget>
			</DialogueOption>
		</DialogueOptionsList>
	</DialogueNode>
	
	<DialogueNode>
		<Name>1</Name>
		<Dialogue>
			<Page>If you're a huge fan, why aren't you blowing air at me?</Page>
			<Page>Haha that's a joke. I'm working on my standup comedy routine.</Page>
		</Dialogue>
		<DialogueOptionsList>
			<DialogueOption>
				<Text>Well played.</Text>
				<DialogueTarget>START</DialogueTarget>
			</DialogueOption>
		</DialogueOptionsList>
	</DialogueNode>

	<DialogueNode>
		<Name>2</Name>
		<Dialogue>
			<Page>Uhh.... something something quantum, quantum quantum...</Page>
		</Dialogue>
		<DialogueOptionsList>
			<DialogueOption>
				<Text>That doesn't make any sens--</Text>
				<DialogueTarget>3</DialogueTarget>
			</DialogueOption>
		</DialogueOptionsList>
	</DialogueNode>

	<DialogueNode>
		<Name>3</Name>
		<Dialogue>
			<Page>Bye bye!</Page>
		</Dialogue>
	</DialogueNode>

</DialogueTree>